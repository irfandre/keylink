// manifest.json
{
    "manifest_version": 3,
    "name": "KeyLink",
    "version": "1.0",
    "description": "Linking keys to links",
    "permissions": ["tabs", "activeTab", "storage","declarativeContent"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            // "16": "icon16.png",
            // "48": "icon48.png",
            "128": "images/logo-color4.png"
        }
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "service_worker": "background.js"
    }
}
