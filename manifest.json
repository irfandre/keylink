// manifest.json
{
  "manifest_version": 3,
  "name": "KeyLink",
  "version": "1.0",
  "description": "Linking keys to links",
  "permissions": ["tabs","activeTab","storage"],
  "action": {

    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
 "background": {
    "service_worker": "background.js"
  }
}
